{
  "name": "eduardochiaro",
  "version": "5.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,json,graphql}\"",
    "cleanup": "npm run prettier && npm run lint:fix",
    "export": "next build && next export",
    "cy:open": "concurrently \"next dev\" \"cypress open\"",
    "cy:run": "concurrently \"next dev\" \"cypress run\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.local prisma migrate dev",
    "prisma:migrate-prod": "dotenv -e .env.local prisma migrate deploy",
    "prisma:migrate-empty-dev": "dotenv -e .env.local prisma migrate dev --create-only",
    "prisma:version": "prisma -v",
    "prisma:seed": "dotenv -e .env.local node prisma/seed/seeder.js"
  },
  "author": {
    "name": "Eduardo Chiaro",
    "email": "e.chiaro@gmail.com",
    "url": "https://www.eduardochiaro.com"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.1.1",
    "@types/node": "^20.4.8",
    "@types/react": "18.2.18",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.2.1",
    "graphql": "^16.7.1",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "nanoid": "^4.0.2",
    "next": "^13.4.12",
    "next-api-middleware": "^2.0.1",
    "next-auth": "^4.22.3",
    "next-progress": "^2.3.1",
    "next-themes": "^0.2.1",
    "node-filesystem-cache": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-inlinesvg": "^3.0.2",
    "react-masonry-css": "^1.0.16",
    "rm-file": "^2.0.0",
    "rss-to-json": "^2.1.1",
    "sass": "^1.64.2",
    "sharp": "^0.32.4",
    "swr": "^2.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/gtag.js": "^0.0.12",
    "@types/mime-types": "^2.1.1",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/typescript-estree": "^6.2.1",
    "concurrently": "^8.2.0",
    "cypress": "^12.17.3",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.4.12",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "prisma": "^5.1.1"
  }
}
