{
  "name": "eduardochiaro",
  "version": "6.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3001",
    "start-dev": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,json,graphql}\"",
    "cleanup": "npm run prettier && npm run lint:fix",
    "cy:open": "concurrently \"next dev\" \"cypress open\"",
    "cy:run": "concurrently \"next dev\" \"cypress run\"",
    "prisma:generate": "prisma generate",
    "prisma:push": "dotenv -e .env.local prisma db push --accept-data-loss && prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.local prisma migrate dev",
    "prisma:migrate-prod": "dotenv -e .env.local prisma migrate deploy",
    "prisma:migrate-empty-dev": "dotenv -e .env.local prisma migrate dev --create-only",
    "prisma:version": "prisma -v",
    "prisma:seed": "dotenv -e .env.local node prisma/seed/seeder.js"
  },
  "author": {
    "name": "Eduardo Chiaro",
    "email": "e.chiaro@gmail.com",
    "url": "https://www.eduardochiaro.com"
  },
  "dependencies": {
    "@apollo/client": "^3.11.1",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^5.17.0",
    "@types/node": "^20.14.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "^18.3.0",
    "arctic": "^1.9.2",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.4.2",
    "graphql": "^16.9.0",
    "lucia": "^3.2.0",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "nanoid": "^5.0.7",
    "next": "^14.2.5",
    "next-api-middleware": "^3.0.0",
    "next-nprogress-bar": "^2.3.13",
    "next-themes": "^0.3.0",
    "node-filesystem-cache": "^1.0.2",
    "oslo": "^1.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-inlinesvg": "^4.1.3",
    "react-masonry-css": "^1.0.16",
    "rm-file": "^2.0.0",
    "rss-to-json": "^2.1.1",
    "sass": "^1.77.8",
    "sharp": "^0.33.4",
    "swr": "^2.2.5",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/gtag.js": "^0.0.20",
    "@types/mime-types": "^2.1.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@typescript-eslint/typescript-estree": "^7.17.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.13.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.17.0"
  }
}
