{
  "name": "eduardochiaro",
  "version": "6.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start -p 3001",
    "start-dev": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,json,graphql}\"",
    "cleanup": "npm run prettier && npm run lint:fix",
    "cy:open": "concurrently \"next dev\" \"cypress open\"",
    "cy:run": "concurrently \"next dev\" \"cypress run\"",
    "prisma:generate": "prisma generate",
    "prisma:push": "dotenv -e .env.local prisma db push --accept-data-loss && prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.local prisma migrate dev",
    "prisma:migrate-prod": "dotenv -e .env.local prisma migrate deploy",
    "prisma:migrate-empty-dev": "dotenv -e .env.local prisma migrate dev --create-only",
    "prisma:version": "prisma -v",
    "prisma:seed": "dotenv -e .env.local node prisma/seed/seeder.js"
  },
  "author": {
    "name": "Eduardo Chiaro",
    "email": "e.chiaro@gmail.com",
    "url": "https://www.eduardochiaro.com"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.5",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^5.21.1",
    "arctic": "^2.0.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.4.2",
    "graphql": "^16.9.0",
    "lucia": "^3.2.2",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "nanoid": "^5.0.7",
    "next": "15.0.1",
    "next-api-middleware": "^3.0.0",
    "next-nprogress-bar": "^2.3.14",
    "next-themes": "^0.3.0",
    "node-filesystem-cache": "^1.0.2",
    "oslo": "^1.2.1",
    "react": "19.0.0-rc-69d4b800-20241021",
    "react-dom": "19.0.0-rc-69d4b800-20241021",
    "react-inlinesvg": "^4.1.4",
    "react-masonry-css": "^1.0.16",
    "rm-file": "^2.0.0",
    "rss-parser": "^3.13.0",
    "sass": "^1.80.4",
    "sharp": "^0.33.5",
    "swr": "^2.2.5",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/gtag.js": "^0.0.20",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.8.1",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@typescript-eslint/typescript-estree": "^8.11.0",
    "concurrently": "^9.0.1",
    "cypress": "^13.15.1",
    "eslint": "^9.13.0",
    "eslint-config-next": "15.0.1",
    "postcss": "^8.4.47",
