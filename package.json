{
  "name": "eduardochiaro",
  "version": "5.3.4",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,json,graphql}\"",
    "cleanup": "npm run prettier && npm run lint:fix",
    "export": "next build && next export",
    "cy:open": "concurrently \"next dev\" \"cypress open\"",
    "cy:run": "concurrently \"next dev\" \"cypress run\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.local prisma migrate dev",
    "prisma:migrate-prod": "dotenv -e .env.local prisma migrate deploy",
    "prisma:push": "dotenv -e .env.local prisma db push --accept-data-loss && prisma generate",
    "prisma:migrate-empty-dev": "dotenv -e .env.local prisma migrate dev --create-only",
    "prisma:version": "prisma -v",
    "prisma:seed": "dotenv -e .env.local node prisma/seed/seeder.js"
  },
  "author": {
    "name": "Eduardo Chiaro",
    "email": "e.chiaro@gmail.com",
    "url": "https://www.eduardochiaro.com"
  },
  "dependencies": {
    "@apollo/client": "^3.8.3",
    "@editorjs/code": "^2.8.0",
    "@editorjs/delimiter": "^1.3.0",
    "@editorjs/editorjs": "^2.28.0",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.8.1",
    "@editorjs/inline-code": "^1.4.0",
    "@editorjs/link": "^2.5.0",
    "@editorjs/list": "^1.8.0",
    "@editorjs/marker": "^1.3.0",
    "@editorjs/paragraph": "^2.10.0",
    "@editorjs/quote": "^2.5.0",
    "@editorjs/raw": "^2.4.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.3.1",
    "@types/node": "^20.6.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.3.0",
    "editorjs-html": "^3.4.3",
    "graphql": "^16.8.0",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "nanoid": "^5.0.1",
    "next": "^13.4.19",
    "next-api-middleware": "^2.0.1",
    "next-auth": "^4.23.1",
    "next-nprogress-bar": "^2.1.2",
    "next-themes": "^0.2.1",
    "node-filesystem-cache": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-inlinesvg": "^4.0.3",
    "react-masonry-css": "^1.0.16",
    "rm-file": "^2.0.0",
    "rss-to-json": "^2.1.1",
    "sass": "^1.67.0",
    "sharp": "^0.32.5",
    "swr": "^2.2.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/gtag.js": "^0.0.13",
    "@types/mime-types": "^2.1.1",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/parser": "^6.7.0",
    "@typescript-eslint/typescript-estree": "^6.7.0",
    "concurrently": "^8.2.1",
    "cypress": "^13.2.0",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1"
  }
}
