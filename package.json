{
  "name": "eduardochiaro",
  "version": "6.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start -p 3001",
    "start-dev": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,json,graphql}\"",
    "cleanup": "npm run prettier && npm run lint:fix",
    "cy:open": "concurrently \"next dev\" \"cypress open\"",
    "cy:run": "concurrently \"next dev\" \"cypress run\"",
    "prisma:generate": "prisma generate",
    "prisma:push": "dotenv -e .env.local prisma db push --accept-data-loss && prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.local prisma migrate dev",
    "prisma:migrate-prod": "dotenv -e .env.local prisma migrate deploy",
    "prisma:migrate-empty-dev": "dotenv -e .env.local prisma migrate dev --create-only",
    "prisma:version": "prisma -v",
    "prisma:seed": "dotenv -e .env.local node prisma/seed/seeder.js"
  },
  "author": {
    "name": "Eduardo Chiaro",
    "email": "e.chiaro@gmail.com",
    "url": "https://www.eduardochiaro.com"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@auth/prisma-adapter": "^2.7.4",
    "@headlessui/react": "^2.2.0",
    "@prisma/client": "^6.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv-cli": "^8.0.0",
    "graphql": "^16.10.0",
    "install": "^0.13.0",
    "lucide-react": "^0.483.0",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "nanoid": "^5.1.5",
    "next": "15.2.3",
    "next-api-middleware": "^3.0.0",
    "next-auth": "^5.0.0-beta.25",
    "next-nprogress-bar": "^2.4.7",
    "next-themes": "^0.4.6",
    "node-filesystem-cache": "^1.0.2",
    "oslo": "^1.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-inlinesvg": "^4.2.0",
    "react-masonry-css": "^1.0.16",
    "rm-file": "^2.0.0",
    "rss-parser": "^3.13.0",
    "sass": "^1.86.0",
    "sharp": "^0.33.5",
    "swr": "^2.3.3",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.2.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/gtag.js": "^0.0.20",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.13.11",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@typescript-eslint/typescript-estree": "^8.27.0",
    "concurrently": "^9.1.2",
    "cypress": "^14.2.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.3.1"
  }
}
