{
  "name": "eduardochiaro",
  "version": "6.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start -p 3001",
    "start-dev": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,json,graphql}\"",
    "cleanup": "npm run prettier && npm run lint:fix",
    "cy:open": "concurrently \"next dev\" \"cypress open\"",
    "cy:run": "concurrently \"next dev\" \"cypress run\"",
    "prisma:generate": "prisma generate",
    "prisma:reset": "dotenv -e .env.local prisma migrate reset",
    "prisma:push": "dotenv -e .env.local prisma db push --accept-data-loss && prisma generate",
    "prisma:studio": "dotenv -e .env.local prisma studio",
    "prisma:migrate-dev": "dotenv -e .env.local prisma migrate dev",
    "prisma:migrate-prod": "dotenv -e .env.local prisma migrate deploy",
    "prisma:migrate-empty-dev": "dotenv -e .env.local prisma migrate dev --create-only",
    "prisma:version": "prisma -v",
    "prisma:seed": "dotenv -e .env.local node prisma/seed/seeder.js"
  },
  "author": {
    "name": "Eduardo Chiaro",
    "email": "e.chiaro@gmail.com",
    "url": "https://www.eduardochiaro.com"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@auth/prisma-adapter": "^2.10.0",
    "@headlessui/react": "^2.2.7",
    "@prisma/client": "^6.13.0",
    "@tailwindcss/postcss": "^4.1.11",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv-cli": "^10.0.0",
    "eslint-plugin-react-hooks": "^0.0.0-experimental-be11cb5c-20250804",
    "graphql": "^16.11.0",
    "lucide-react": "^0.536.0",
    "mime-types": "^3.0.1",
    "moment": "^2.30.1",
    "nanoid": "^5.1.5",
    "next": "15.4.5",
    "next-auth": "^5.0.0-beta.26",
    "next-nprogress-bar": "^2.4.7",
    "next-themes": "^0.4.6",
    "node-filesystem-cache": "^1.0.2",
    "react": "^0.0.0-experimental-be11cb5c-20250804",
    "react-dom": "^0.0.0-experimental-be11cb5c-20250804",
    "react-inlinesvg": "^4.2.0",
    "react-masonry-css": "^1.0.16",
    "rss-parser": "^3.13.0",
    "sass": "^1.89.2",
    "sharp": "^0.34.3",
    "swr": "^2.3.4",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.4.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/gtag.js": "^0.0.20",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@typescript-eslint/typescript-estree": "^8.39.0",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.3",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.13.0"
  }
}
