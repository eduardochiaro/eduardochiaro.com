    "@types/mime-types": "^2.1.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/parser": "^6.19.0",
    "@typescript-eslint/typescript-estree": "^6.19.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.8.1"
  }
