{
  "name": "eduardochiaro",
  "version": "5.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write \"src/**/*.{js,json,graphql}\"",
    "cleanup": "npm run prettier && npm run lint:fix",
    "export": "next build && next export",
    "cypress": "concurrently \"next dev\" \"cypress open\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.local prisma migrate dev",
    "prisma:migrate-prod": "dotenv -e .env.local prisma migrate deploy",
    "prisma:migrate-empty-dev": "dotenv -e .env.local prisma migrate dev --create-only",
    "prisma:version": "prisma -v",
    "prisma:seed": "dotenv -e .env.local node seed/seeder.js"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.12",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.4.0",
    "@types/react": "^18.0.21",
    "axios": "^1.1.2",
    "case": "^1.6.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv-cli": "^6.0.0",
    "formidable": "^2.0.1",
    "graphql": "^16.6.0",
    "moment": "^2.29.4",
    "mv": "^2.1.1",
    "nanoid": "^4.0.0",
    "next": "^12.3.1",
    "next-api-middleware": "^2.0.1",
    "next-auth": "^4.13.0",
    "next-progress": "^2.2.0",
    "next-themes": "^0.2.1",
    "node-filesystem-cache": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-inlinesvg": "^3.0.1",
    "react-masonry-css": "^1.0.16",
    "react-xmasonry": "^3.0.6",
    "rm-file": "^2.0.0",
    "rss-parser": "^3.12.0",
    "sass": "^1.55.0",
    "sharp": "^0.31.1",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "cypress": "^10.9.0",
    "eslint": "^8.25.0",
    "eslint-config-next": "^12.3.1",
    "postcss": "^8.4.17",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "tailwindcss": "^3.1.8"
  }
}
