{
  "name": "eduardochiaro",
  "version": "5.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write \"src/**/*.{js,json,graphql}\"",
    "cleanup": "npm run prettier && npm run lint:fix",
    "export": "next build && next export",
    "cy:open": "concurrently \"next dev\" \"cypress open\"",
    "cy:run": "concurrently \"next dev\" \"cypress run\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.local prisma migrate dev",
    "prisma:migrate-prod": "dotenv -e .env.local prisma migrate deploy",
    "prisma:migrate-empty-dev": "dotenv -e .env.local prisma migrate dev --create-only",
    "prisma:version": "prisma -v",
    "prisma:seed": "dotenv -e .env.local node seed/seeder.js"
  },
  "author": {
    "name": "Eduardo Chiaro",
    "email": "e.chiaro@gmail.com",
    "url": "https://www.eduardochiaro.com"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@headlessui/react": "^1.7.12",
    "@heroicons/react": "^2.0.16",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.10.1",
    "axios": "^1.3.4",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.0.0",
    "formidable": "^2.1.1",
    "graphql": "^16.6.0",
    "https-proxy-agent": "^5.0.1",
    "moment": "^2.29.4",
    "mv": "^2.1.1",
    "nanoid": "^4.0.1",
    "next": "^13.2.1",
    "next-api-middleware": "^2.0.1",
    "next-auth": "^4.19.2",
    "next-progress": "^2.2.0",
    "next-themes": "^0.2.1",
    "node-filesystem-cache": "^1.0.2",
    "react-inlinesvg": "^3.0.2",
    "react-masonry-css": "^1.0.16",
    "react-scrollspy-nav": "^1.3.0",
    "rm-file": "^2.0.0",
    "rss-parser": "^3.12.0",
    "sass": "^1.58.3",
    "sharp": "^0.31.3",
    "swr": "^2.0.4",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/gtag.js": "^0.0.12",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cypress": "^12.6.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "^13.2.1",
    "madge": "^6.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.7"
  }
}
